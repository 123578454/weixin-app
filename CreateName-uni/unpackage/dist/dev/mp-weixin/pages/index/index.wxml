<view class="content"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="cu-form-group"><view class="title">宝宝姓氏</view><input name="input" placeholder="请输入姓氏" maxlength="2"/></view><view class="cu-form-group"><view class="title">宝宝性别</view><radio-group name="radio1"><label><radio class="{{['black radio',radio=='A'?'checked':'']}}" checked="{{radio=='A'?true:false}}" value="A"></radio><text>男孩</text></label><label><radio class="{{['black radio',radio=='B'?'checked':'']}}" checked="{{radio=='B'?true:false}}" value="B"></radio><text>女孩</text></label></radio-group></view><view class="cu-form-group"><view class="title">名字字数</view><radio-group name="radio2"><label><radio class="{{['black radio',radio=='C'?'checked':'']}}" checked="{{radio=='C'?true:false}}" value="C"></radio><text>单字名</text></label><label><radio class="{{['black radio',radio=='D'?'checked':'']}}" checked="{{radio=='D'?true:false}}" value="D"></radio><text>多字名</text></label></radio-group></view><view class="cu-form-group"><view class="title">出生日期</view><picker name="datePicker" mode="date" value="{{date}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{date}}</view></picker></view><view class="cu-form-group"><view class="title">出生时辰</view><picker name="timePicker" mode="time" value="{{time}}" start="09:01" end="21:01" data-event-opts="{{[['change',[['bindTimeChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{time}}</view></picker></view><view class="margin"><button class="w-btn big radius bg-black margin-tb white-color" form-type="submit">免费起名</button></view></form></view>